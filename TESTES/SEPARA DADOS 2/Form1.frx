 A.CODREL, 
 CASE 
     WHEN A.FCE = 0 THEN 
         NULL 
     ELSE 
         A.FCE 
 END AS FCE, 
 B.PROJETO, 
 B.DESCRICAO, 
 A.DATAREL, 
 CASE 
     WHEN A.STATUSIMP = 0 THEN 
         'NÃO IMPRESSO' 
     ELSE 
         'IMPRESSO' 
 END, 
 CASE 
     WHEN C.STATUS = 0 THEN 
         'ANDAMENTO' 
     WHEN C.STATUS = 1 THEN 
         'CONCLUIDA' 
     WHEN C.STATUS IS NULL THEN 
         'DUVIDA' 
     WHEN C.STATUS = 2 THEN 
         'PARALIZADA' END 
 AS STATUS 
FROM TBRELINSPEXP AS A 
LEFT JOIN TBPROJETOS AS B ON A.FCE = B.FCE AND A.CODPROJETO = B.CODPROJETO 
LEFT JOIN TBFCE AS C ON B.FCE = C.FCE 
WHERE A.TIPOREL = 11
